{
  "kind": "collectionType",
  "collectionName": "customers",
  "info": {
    "singularName": "customer",
    "pluralName": "customers",
    "displayName": "Customer",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "phone": {
      "type": "uid",
      "required": true
    },
    "email": {
      "type": "email"
    },
    "password": {
      "type": "password"
    },
    "alt_phone": {
      "type": "integer"
    },
    "is_verified": {
      "type": "boolean"
    },
    "addresses": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::address.address",
      "mappedBy": "customer"
    },
    "cart": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::cart.cart",
      "mappedBy": "customer"
    },
    "wishlist": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::wishlist.wishlist",
      "mappedBy": "customer"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "customer"
    }
  }
}
