{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "Order"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "customer": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::customer.customer",
      "inversedBy": "orders"
    },
    "products": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product.product"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "PROCESSING",
        "PENDING",
        "CONFIRMED",
        "FAILED",
        "CANCELED",
        "OUTFORDELIVERY",
        "DELIVERED",
        "OUTFORPICKUP",
        "RETURNED"
      ]
    },
    "total": {
      "type": "integer"
    },
    "applied_coupon": {
      "type": "string"
    },
    "discount": {
      "type": "decimal"
    },
    "payment_status": {
      "type": "enumeration",
      "enum": [
        "PENDING",
        "FAILED",
        "COMPLETED",
        "REVERSED"
      ]
    },
    "payment_mode": {
      "type": "string"
    },
    "address": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::address.address"
    },
    "transaction_id": {
      "type": "string"
    },
    "transaction_response": {
      "type": "string"
    },
    "pushed_to_ecogreen": {
      "type": "boolean"
    }
  }
}
